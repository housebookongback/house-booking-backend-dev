{
  "name": "house-booking-express",
  "version": "1.0.0",
  "description": "Back-end API for a house booking application (Express + Sequelize)",
  "main": "src/server.js",
  "private": true,
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "db:init": "sequelize-cli init",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "node src/seed/index.js",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mailersend": "^2.5.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "slugify": "^1.6.6",
    "twilio": "^5.6.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.0",
    "sequelize-cli": "^6.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT"
}
